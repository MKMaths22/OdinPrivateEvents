<% events_to_display.each do |event| %>
  Event
  <%= event.id %>
  <br>
  Takes place at
  <%= event.location %>
  <br>
  At 
  <%= event.date %>
  <br>
  <% if link_single_events %>
    <%= link_to "More info on this Event", event_path(id: event.id) %>
  <% end %>
  <% if current_user %>
    <% if current_user.attended_events.include?(event) %>
      You are booked to Attend this Event
      <% else %>
      <%= link_to "Attend this Event", bookings_path(event_id: event.id), data: { 'turbo-method' => :post } %>
    <% end %>
  <% end %>
  <br>
  <br>
<% end %>